using System;
using Terraria;

namespace ChemistryClass.Tiles.Blocks {
    public class SulfuricOreHeartTile : SulfuricOreTile {

        public override void SetDefaults() {

            base.SetDefaults();
            this.mineResist = 16f;
            Main.tileTexture[Type] = Main.tileTexture[base.Type];

        }

    }
}
